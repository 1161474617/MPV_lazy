//!DESC (AU_F_4x_P) aggregation
//!BIND SUBCONV1
//!BIND SUBCONV2
//!BIND SUBCONV3
//!BIND SUBCONV4
vec4 hook()
{
vec2 fcoord = fract(SUBCONV1_pos * SUBCONV1_size);
vec2 base = SUBCONV1_pos + (vec2(0.5) - fcoord) * SUBCONV1_pt;
ivec2 index = ivec2(fcoord * vec2(4));
mat4 res = mat4(SUBCONV1_tex(base).rgba,SUBCONV2_tex(base).rgba,SUBCONV3_tex(base).rgba,SUBCONV4_tex(base).rgba);
